# location
COUNTRY="Norway"
REGION="Oslo"
CITY="Oslo/Majorstuen"
LAT="59.992"
LON="10.72"

# settings
WLAN_DEVICE=wlan0 # wlan device
LAN_DEVICE=enp3s0f4u1u4
BATTERY=BAT1

# colors
BG=#00171719
FG=#F5F5F5
MAGENTA=#C792C1
PURPLE=#470e34
GRAY=#888888
DARKGRAY=#232c32
CYAN=#239fa8
RED=#e52d4c
ORANGE=#ff9540

ICON_COLOR=$CYAN
ICON_COLOR_WARN=$RED

# dimensions
SEP=8
HEIGHT=28
SCREENWIDTH=`xrandr | grep \* | awk '{split($1,w,"x"); print w[1]}' | sed -n 1p`
WIDTH=1200
X=$(( SCREENWIDTH/2-WIDTH/2 ))
Y=$SEP
DIMENSIONS=`printf "%dx%d+%d+%d" $WIDTH $HEIGHT $X $Y`

# fonts
FONTS=(
	"neep-10"
	"-wuncon-siji-medium-r-normal--10-100-75-75-c-80-iso10646-1"
